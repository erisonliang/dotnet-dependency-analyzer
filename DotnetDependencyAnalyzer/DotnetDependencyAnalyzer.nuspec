<?xml version="1.0"?>
<package >
  <metadata>
    <id>DotnetDependencyAnalyzer</id>
    <version>1.2.0-beta</version>
    <title>.NET Dependency Analyzer</title>
    <authors>Hugo Rocha</authors>
    <owners>$pt-osda</owners>
    <licenseUrl>https://opensource.org/licenses/BSD-3-Clause</licenseUrl>
    <projectUrl>https://github.com/pt-osda/dotnet-dependency-analyzer</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Analyzes dependencies, concerning their licenses and vulnerabilities, used in projects developed in .NET platform</description>
    <releaseNotes>Reads license url from packages and evaluates packages according to their vulnerabilities. Generates report and stores it on a database.</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags>Dotnet Plugin Vulnerabilities Licenses</tags>
	<dependencies>
		<dependency id="NuGet.Packaging" version="4.5.0" />
		<dependency id="Newtonsoft.Json" version="9.0.1" />
	</dependencies>
  </metadata>
  <files>
	<file src="..\DependencyAnalyzer.exe" target="tools/DependencyAnalyzer.exe" />
    <file src="bin\Release\DotnetDependencyAnalyzer.exe" target="lib\DotnetDependencyAnalyzer.exe" />
  </files>
</package>